<launch>
    <!-- Launch gmapping, loading configuration file -->
    <node pkg="gmapping" type="slam_gmapping" name="gmapping" output="screen" >
       <remap from="scan" to="p_spombinho/scan" />
       <param name="odom_frame" value="p_spombinho/odom" />
       <param name="base_frame" value="p_spombinho/base_footprint" />
    </node>
          
</launch>
